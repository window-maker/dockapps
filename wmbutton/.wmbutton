26	xterm -geometry 120x35 -fg black -bg grey70 -T 'WMButton Config Editor' -e pico -w ~/.wmbutton &

7	gimp &
17	xfig &
27	kpaint &

8	kghostview &
18	ggv &
28	xpdf &

