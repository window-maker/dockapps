11	mozilla -mail &
21	firefox &

2	xterm -geometry 80x35 -sb -fg white -bg grey20 -T 'Xterm: User' &
12	xterm -geometry 80x35 -sb -fg snow -bg midnightblue -T 'Xterm: Root' &
